{
  "language": "en",
  "version": "1.1.1",
  "title": "HelloSignApi",
  "description": "Unofficial task-based HelloSign api lib for dotnet with intellisense doc.",
  "authors": [ "Yin-Chun Wang" ],
  "copyright": "Copyright (C) Yin-Chun Wang 2016-2017",
  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/soukoku/HelloSignApi/master/LICENSE",
    "projectUrl": "https://github.com/soukoku/HelloSignApi",
    "requireLicenseAcceptance": false,
    "tags": [ "eSignature" ]
  },
  "buildOptions": {
    "xmlDoc": true,
    "keyFile": "Sign.snk",
    "warningsAsErrors": true
  },
  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "net40-client": {
      "buildOptions": {
        "define": [ "FULLFX" ]
      },
      "dependencies": {
        "Microsoft.Net.Http": "2.2.29"
      },
      "frameworkAssemblies": {
        "PresentationCore": "",
        "System.Drawing": ""
      }
    },
    "net45": {
      "buildOptions": {
        "define": [ "FULLFX" ]
      },
      "dependencies": {
      },
      "frameworkAssemblies": {
        "PresentationCore": "",
        "System.Drawing": "",
        "System.Net.Http": ""
      }
    },
    "netstandard1.3": {
      "dependencies": {
        //"NETStandard.Library": "1.6.0"
        "System.Collections.Concurrent": "4.3.0",
        "System.IO": "4.3.0",
        "System.IO.FileSystem": "4.3.0",
        "System.IO.FileSystem.Primitives": "4.3.0",
        "System.Linq": "4.3.0",
        "System.Net.Http": "4.3.0",
        "System.Resources.ResourceManager": "4.3.0",
        "System.Runtime": "4.3.0",
        "System.Runtime.Extensions": "4.3.0",
        "System.Runtime.InteropServices": "4.3.0",
        "System.Security.Cryptography.Algorithms": "4.3.0",
        "System.Text.RegularExpressions": "4.3.0"
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "buildOptions": {
        "define": [ "PORTABLE" ]
      },
      "dependencies": {
        "Microsoft.Net.Http": "2.2.29"
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "buildOptions": {
        "define": [ "PORTABLE" ]
      },
      "dependencies": {
        "Microsoft.Net.Http": "2.2.29"
      },
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Collections": "",
        "System.Core": "",
        "System.Linq": "",
        "System.Net.Primitives": "",
        "System.Resources.ResourceManager": "",
        "System.Runtime.Extensions": "",
        "System.Text.Encoding": "",
        "System.Text.RegularExpressions": "",
        "System.Threading": ""
      }
    }
    //"uap": {
    //  "buildOptions": {
    //    "define": [ "WINDOWS_UWP" ]
    //  },
    //  "dependencies": {
    //    "Microsoft.NETCore.UniversalWindowsPlatform": "5.0.0"
    //  }
    //}
  }
}
